<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
        img {
            width: 300px;
            height: 300px;
            object-fit: cover;
        }
    </style>
  </head>
  <body>
    <h3>This page belongs to @ <%= data.name %></h3>
    <button>Follow</button>
    <button>Message</button>

    <p>
      Followers: <%= data.followers %> &nbsp;&nbsp;&nbsp;&nbsp; Following: <%=
      data.following %>
    </p>
    <hr />
    <% for (let post of data.posts) { %>
      <img src="<%= post.image %>" alt="some image">
        <p><%= post.likes %> Likes&nbsp;&nbsp;&nbsp;&nbsp; <%= post.comments %> Comments</p>
        <hr />
    <% } %>
  </body>
</html>
